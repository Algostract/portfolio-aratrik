{
  "name": "portfolio-artarik",
  "description": "Aratrik's portfolio showcasing his projects, experience and contacts in a visually appealing manner",
  "author": "Shirsendu Bairagi <shirsendu2001@gmail.com>",
  "version": "1.6.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.15.0"
  },
  "license": "LGPL-3.0-or-later",
  "scripts": {
    "postinstall": "nuxt prepare",
    "dev": "nuxt dev",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier .  --write",
    "build": "nuxt build && cp -r static .output/static",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "start": "node .output/server/index.mjs",
    "tunnel": "untun tunnel --port 1002 http://localhost:1002",
    "clean": "nuxt cleanup",
    "docker:build": "docker build --build-arg TAG=dev -t portfolio-aratrik:dev .",
    "docker:start": "docker run --detach --name portfolio-aratrik-dev --mount type=bind,source=$(pwd)/static,target=/app/static,readonly --env-file .env.prod -p 1002:3000 portfolio-aratrik:dev"
  },
  "dependencies": {
    "confbox": "^0.1.7",
    "vue": "latest",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.7",
    "@nuxt/eslint": "^0.5.0",
    "@nuxt/fonts": "^0.7.2",
    "@nuxt/image": "^1.7.0",
    "@nuxt/test-utils": "^3.14.1",
    "@nuxtjs/color-mode": "^3.4.2",
    "@nuxtjs/seo": "^2.0.0-rc.18",
    "@nuxtjs/tailwindcss": "^6.12.1",
    "@vite-pwa/nuxt": "^0.9.1",
    "@vueuse/nuxt": "^10.11.1",
    "eslint-config-prettier": "^9.1.0",
    "nuxt": "^3.12.4",
    "nuxt-gtag": "^2.1.0",
    "nuxt-icons": "^3.2.1",
    "nuxt-splide": "^1.1.1",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "untun": "^0.1.3"
  }
}